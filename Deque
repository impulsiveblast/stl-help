#include<iostream>
#include<deque>  //including its library or use  #include<bits/stdc++.h> elements can be inserted in both start and end
using namespace std;

//implementing vector using stl functions

int main()
{
  
  deque<int> a;
  
  a.push_back(1); //inserting element from back/end
  a.push_front(2);//inserting element from start/front
  
  a.pop_back();   //element from back gets deleted
  a.pop_front();  //element fro front gets deteted
  
  a.push_back(1); //inserting element from back/end
  a.push_front(2);//inserting element from start/front
  
  cout<<a.at(1)<<endl; //element at index 1 gets printed
  
  
  for(int i:a) //elements of deque get printed
  {
      cout<<i<<endl;
  }
  
  
  return 0;

    
}
  

#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    
    bool is_prime=true;
    
    for(int i=2;i*i<=n;i++)   //only iterating till the square root of a number so that length of checking prime factors would be less
    {
       if(n%i==0)
       {
          is_prime=false; 
          break;
       }
    }
    
    cout<<is_prime;
    
    return 0;
}


    #include<bits/stdc++.h>
    using namespace std;
    
    int main()
    {
        priority_queue<int> pq //default max heap
        
        pq.push(1);
        pq.push(2);
        pq.push(4);
        pq.push(7); //7421  descending order
        
        pq.size();
        
        while(!pq.empty())
        {
            int val=pq.top();
            pq.pop();
            cout<<val<<" ";
        }
        
        //priority_queue<int,vector<int>,greater<int> > pq   :min heap
        //arranges elements in ascending order
        
        return 0;
        
    }
